const x=1;

switch (x) {
  case 1:
    console.log(1);
    break;
  case 2:
    console.log(2);
    break;
  default:
    console.log("Тип x не определен")
}




const x=NaN;

switch (x) {
  case 1:
    console.log(1);
    break;
  case 2:
    console.log(2);
    break;
  default:
    console.log("Тип x не определен")
}

const x=null;

switch (x) {
  case 1:
    console.log(1);
    break;
  case 2:
    console.log(2);
    break;
  default:
    console.log("Тип x не определен")
}
